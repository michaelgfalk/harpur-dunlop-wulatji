@article{ackland_charles_1983,
	title = {Charles {Harpur}'s "the bush fire" and "a storm in the mountains": {Sublimity}, cognition and faith},
	volume = {43},
	issn = {0038-3732},
	abstract = {Charles Harpur's landscape verse is now widely appreciated, but it is still far from being completely understood. Commentary has noted its emphasis on elemental nature and on human presence, and that the individual scene tends to assume a symbolic dimension. We lack, however, a cogent theory which interrelates these characteristics and indicates their place in the poet's self-avowed mission to be "the Bard of thy Country". The solution, I believe, lies in the concept of sublimity, which informs such representative Harpur works as "The Bush Fire" and "A Storm in the Mountains". These poems, in all their essential features, proclaim their adherence to the canon of the eighteenth-century sublime. Their language, themes and narrative unfolding recall Milton, Wordsworth and the eighteenth-century descriptive verse tradition, while their concentration on the awesome aspects of nature, and on our emotional response to them, would have found favour with such theorists as John Dennis and Edmund Burke. But even more telling is the interest these works evidence in cognitive processes. For by the end of the eighteenth century, the locus of sublimity had shifted from vast natural phenomena to the perceiving mind; and poets had become increasingly preoccupied with that interaction between external objects and intellect which creates our sense of grandeur and meaning. In what follows, I shall concentrate on two neglected aspects of "The Bush Fire" and "A Storm in the Mountains": their concern with the relationship between nature and mind, and between human and supernatural elements. First I wish to demonstrate the highly conscious strategies underlying their presentation of these motifs. Then, in the final section, I shall argue that these familiar features of sublime verse are indices, not of unthinking assimilation, but of an attempt to infuse the Burkean aesthetic of terror with broader issues of moral and national consequence.},
	number = {4},
	journal = {Southerly},
	author = {Ackland, Michael},
	month = dec,
	year = {1983},
	keywords = {Harpur, Charles, 1813-1868, Australia, Manuscripts},
	pages = {459--474},
	file = {Ackland_BushFire:/Users/falk/Zotero/storage/VP2439FY/Ackland_BushFire.pdf:application/pdf},
}


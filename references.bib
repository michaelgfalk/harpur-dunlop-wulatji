	author = {Falk, Michael},
	month = feb,
	year = {2019},
	keywords = {Charles Harpur, colonial poetry, colonial politics},
	file = {Full Text PDF:/Users/falk/Zotero/storage/5YUPTKKP/Falk - 2019 - The Endless Forms of Things Harpur's Radicalism R.pdf:application/pdf;Snapshot:/Users/falk/Zotero/storage/FBKAWXZJ/13026.html:text/html},
}


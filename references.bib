@article{ackland_charles_1984-1,
	title = {Charles {Harpur} for a new generation},
	volume = {28},
	issn = {0033-5002},
	abstract = {Charles Harpur's reputation as a poet rests largely on the pictorial qualities of his works. Editions of his verse highlight his achievement in landscape depiction, and commentary on his poems has revealed their affinities to eighteenth- century, categories of the sublime and the picturesque. Moreover Harpur himself, in "The Dream by the Fountain", appears to lend tacit support to this mimetic orientation, when he links his poetic vocation with the need to celebrate "Scenes that spread recordless round thee suffusing". Thus his verse is either praised for its accurate observation and for its painterly presentation of detail; or censured for its departures from straightforward landscape presentation and for its dependence on poetic precursors. Yet the standard of verisimilitude implied by these judgments seems itself to be curiously anachronistic and fraught with potential misunderstandings. While early painters of Australian landscape may have sought empirical accuracy, colonial poets were not burdened with the task of scientific observation. On the contrary, they found themselves heirs of a verse tradition which had become increasingly reflective, or conscious of the shaping power of the individual mind. By the time Harpur began to publish regularly in 1833, descriptive poetry was concerned of necessity with the interaction of nature and intellect. We should, then, hardly expect to find in his poems objective renderings of natural scenes. Instead, as I hope to demonstrate, Harpur's preoccupations and poetic techniques place him firmly in the tradition of Romantic meditative verse. But first it will be necessary to establish briefly the continuity of his work with major developments in eighteenth-century English poetry, before attempting a more detailed appraisal of his prophetic mission and its consequences for the portrayal of landscape.},
	number = {6},
	journal = {Quadrant},
	author = {Ackland, Michael},
	month = jun,
	year = {1984},
	keywords = {Poetry, Harpur, Charles, 1813-1868, Romanticism},
	pages = {65--71},
	file = {Ackland_NewGeneration:/Users/falk/Zotero/storage/C3FAP7QU/Ackland_NewGeneration.pdf:application/pdf},
}

